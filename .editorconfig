# @see     http://editorconfig.org/
# @version 0.2
# @date    2015-11-02

# This is the top-most .editorconfig file; do not search in parent directories.
root = true

# All files.
[*]
charset = utf-8
end_of_line = LF
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
